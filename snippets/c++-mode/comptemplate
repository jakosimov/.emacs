# -*- mode: snippet -*-
# name: template
# key: :template
# --

#include <bits/stdc++.h>

using namespace std;

typedef long long ll;
typedef std::pair<ll,ll> Pair;
typedef vector<ll> vi;
typedef vector<Pair> vii;

#define rep(i, a, b) for (ll i = a ; i < b ; i++)
#define iter(i, arr) for (ll i = 0 ; i < arr.size() ; i++)
#define trav(a, arr) for (auto a : arr)
#define all(arr) arr.begin(), arr.end()
#define sz(arr) (ll)(arr).size()
#ifdef dbg
#define print(x); cout << x << endl;
#else
#define print(x);
#endif

string arrString(vi arr) {
  string result;
  result += "{ ";
  iter(i, arr) {
    result += to_string(arr[i]);
    if (i != sz(arr)-1) {
      result += ", ";
    }
  }
  result += " }";
  return result;
}

#define LL_INF 9223372036854775807ll

int main() {
  $0

  return 0;
}



